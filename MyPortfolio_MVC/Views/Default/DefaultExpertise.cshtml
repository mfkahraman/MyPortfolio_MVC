@model List<MyPortfolio_MVC.Models.Expertise>

<div class="row about-expertise" data-animate-block id="expertise">
    <div class="column lg-12">

        <h2 class="text-pretitle with-line" data-animate-el>UZMANLIK ALANLARIM</h2>

        <!-- Skills Container -->
        <div class="skills-container" data-animate-el>
            @foreach (var item in Model)
            {
                <div class="skill-card">
                    <h3>@item.Title</h3>
                </div>
            }
        </div>

    </div>
</div>